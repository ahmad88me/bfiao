    <!-- http://localhost:8080/BFiaO/StrategicSector#SectorCode -->


    <owl:DatatypeProperty rdf:about="http://localhost:8080/BFiaO/StrategicSector#SectorCode">
        <rdfs:comment xml:lang="en">It identifies univocally a sector with a number and several letters with the name of the specfic sector.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">It identifies univocally a sector with a number and several letters with the name of the specfic sector.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">SectorCode</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost:8080/BFiaO/StrategicSector#SectorCompetentOrganization -->


    <owl:DatatypeProperty rdf:about="http://localhost:8080/BFiaO/StrategicSector#SectorCompetentOrganization">
        <rdfs:comment xml:lang="en">It identifies the name of the organization (ministry or organism) which is competent in a specific sector or domain from the point of view of emergencies.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">It identifies the name of the organization (ministry or organism) which is competent in a specific sector or domain from the point of view of emergencies.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">SectorCompetentOrganization</rdfs:label>
    </owl:DatatypeProperty>
    


