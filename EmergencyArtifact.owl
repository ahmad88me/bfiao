    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactBedCapability -->

    <owl:DatatypeProperty rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactBedCapability">
        <rdfs:comment xml:lang="en">It defines the amount of bed that a hospital or an evacuted camp can provide.</rdfs:comment>
        <rdfs:label xml:lang="en">ArtifactBedCapability</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactDescription -->

    <owl:DatatypeProperty rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactDescription">
        <rdfs:comment xml:lang="en">It provides a description of a specific artifact</rdfs:comment>
        <rdfs:label xml:lang="en">ArtifactDescription</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactDiggingDepth -->

    <owl:DatatypeProperty rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactDiggingDepth">
        <rdfs:comment xml:lang="en">It is the maximum capacity of digging for an excavator. It is expressed in meters.</rdfs:comment>
        <rdfs:label xml:lang="en">ArtifactDiggingDepth</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactEnginePower -->

    <owl:DatatypeProperty rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactEnginePower">
        <rdfs:comment xml:lang="en">It specifies the engine power of a vehicle in HPA.</rdfs:comment>
        <rdfs:label xml:lang="en">ArtifactEnginePower</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactID -->

    <owl:DatatypeProperty rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactID">
        <rdfs:comment xml:lang="en">This filed identifies univocally any kind of artifact.</rdfs:comment>
        <rdfs:label xml:lang="en">ArtifactID</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactInjuredTransport -->

    <owl:DatatypeProperty rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactInjuredTransport">
        <rdfs:comment xml:lang="en">It defines the ability to transport injured people.</rdfs:comment>
        <rdfs:label xml:lang="en">ArtifactInjuredTransport</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactModel -->

    <owl:DatatypeProperty rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactModel">
        <rdfs:comment xml:lang="en">It identifies the model used for an specific artifact.</rdfs:comment>
        <rdfs:label xml:lang="en">ArtifactModel</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactOperatingStaffSize -->

    <owl:DatatypeProperty rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactOperatingStaffSize">
        <rdfs:comment xml:lang="en">It identifies the level of the staff needed to operate an specific system or equipment.</rdfs:comment>
        <rdfs:label xml:lang="en">ArtifactOperatingStaffSize</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactPersonnelTransportCapacity -->

    <owl:DatatypeProperty rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactPersonnelTransportCapacity">
        <rdfs:comment xml:lang="en">It identifies the capacity for transport of personnel in a vehicle. If the vehicle is an ambulance it identifies the maximum capacity for transporting injured people.</rdfs:comment>
        <rdfs:label xml:lang="en">ArtifactPersonnelTransportCapacity</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactPumpingWaterCapability -->

    <owl:DatatypeProperty rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactPumpingWaterCapability">
        <rdfs:comment xml:lang="en">It defines the amount of water or mud that a pumping equipment can extract. , expressed in cubic meters per hour.</rdfs:comment>
        <rdfs:label xml:lang="en">ArtifactPumpingWaterCapability</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactRange -->

    <owl:DatatypeProperty rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactRange">
        <rdfs:comment xml:lang="en">It is the time, in hours, that an artifact can be working without providing any kind of fuel.</rdfs:comment>
        <rdfs:label xml:lang="en">ArtifactRange</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactSaltCapability -->

    <owl:DatatypeProperty rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactSaltCapability">
        <rdfs:comment xml:lang="en">It defines the maximum amount of salt that a truck or a snowplow can transport. It is expressed im cubic meters.</rdfs:comment>
        <rdfs:label xml:lang="en">ArtifactSaltCapability</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactTipperCapacity -->

    <owl:DatatypeProperty rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactTipperCapacity">
        <rdfs:comment xml:lang="en">It is the capacity of a tipper in a dump truck. It is expressed in cubic meters.</rdfs:comment>
        <rdfs:label xml:lang="en">ArtifactTipperCapacity</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactUnitOwner -->

    <owl:DatatypeProperty rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactUnitOwner">
        <rdfs:comment xml:lang="en">It identifies the unit to which a specific artifact belongs.</rdfs:comment>
        <rdfs:label xml:lang="en">ArtifactUnitOwner</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactWaterCapability -->

    <owl:DatatypeProperty rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#ArtifactWaterCapability">
        <rdfs:comment xml:lang="en">It defines the amount of water that a firefighting vehicle o a cistern can transport, expressed in liters (l).</rdfs:comment>
        <rdfs:label xml:lang="en">ArtifactWaterCapability</rdfs:label>
    </owl:DatatypeProperty>
    


