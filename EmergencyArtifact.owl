    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#aerialVehicleArtifact -->

    <owl:Class rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#aerialVehicleArtifact">
        <rdfs:subClassOf rdf:resource="http://localhost:8080/BFiaO/EmergencyArtifact#emergencyVehicleArtifact"/>
        <rdfs:comment xml:lang="en">an aeroplane, helicopter, or other machine capable of flight</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">an aeroplane, helicopter, or other machine capable of flight</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">aerialVehicleArtifact</rdfs:label>
    </owl:Class>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#ambulanceHelicopterArtifact -->

    <owl:Class rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#ambulanceHelicopterArtifact">
        <rdfs:subClassOf rdf:resource="http://localhost:8080/BFiaO/EmergencyArtifact#rotaryWingArtifact"/>
        <rdfs:comment xml:lang="en">It is a helicopter  designed to providing an ambulance service for those urgent and life-threatening health conditions.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">It is a helicopter  designed to providing an ambulance service for those urgent and life-threatening health conditions.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">ambulanceHelicopterArtifact</rdfs:label>
    </owl:Class>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#blanketArtifact -->

    <owl:Class rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#blanketArtifact">
        <rdfs:subClassOf rdf:resource="http://localhost:8080/BFiaO/EmergencyArtifact#fieldArtifact"/>
        <rdfs:comment xml:lang="en">A blanket is a a flat cover made of wool or similar warm material, usually used on a bed or in emergencies to warm a person.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">A blanket is a a flat cover made of wool or similar warm material, usually used on a bed or in emergencies to warm a person.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">blanketArtifact</rdfs:label>
    </owl:Class>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#bodyBagArtifact -->

    <owl:Class rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#bodyBagArtifact">
        <rdfs:subClassOf rdf:resource="http://localhost:8080/BFiaO/EmergencyArtifact#fieldArtifact"/>
        <rdfs:comment xml:lang="en">A body bag is a large zippered bag (as of rubber or vinyl) in which a human corpse is placed especially for transportation.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">A body bag is a large zippered bag (as of rubber or vinyl) in which a human corpse is placed especially for transportation.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">bodyBagArtifact</rdfs:label>
    </owl:Class>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#bridgeLaunchVehicleArtifact -->

    <owl:Class rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#bridgeLaunchVehicleArtifact">
        <rdfs:subClassOf rdf:resource="http://localhost:8080/BFiaO/EmergencyArtifact#groundVehicleArtifact"/>
        <rdfs:comment xml:lang="en">A bridge launch vehicle is a combat support vehicle, sometimes regarded as a subtype of military engineering vehicle, designed to assist militaries in rapidly deploying tanks and other armoured fighting vehicles across gap-type obstacles, such as (and primarily) rivers. In emergency situations can help to reover from destroyed bridges or road infrastructures.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">A bridge launch vehicle is a combat support vehicle, sometimes regarded as a subtype of military engineering vehicle, designed to assist militaries in rapidly deploying tanks and other armoured fighting vehicles across gap-type obstacles, such as (and primarily) rivers. In emergency situations can help to reover from destroyed bridges or road infrastructures.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">bridgeLaunchVehicleArtifact</rdfs:label>
    </owl:Class>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#bulldozerArtifact -->

    <owl:Class rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#bulldozerArtifact">
        <rdfs:subClassOf rdf:resource="http://localhost:8080/BFiaO/EmergencyArtifact#groundVehicleArtifact"/>
        <rdfs:comment xml:lang="en">A bulldozer is a tractor-driven machine usually having a broad horizontal blade for moving earth.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">A bulldozer is a tractor-driven machine usually having a broad horizontal blade for moving earth.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">bulldozerArtifact</rdfs:label>
    </owl:Class>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#bunkbedArtifact -->

    <owl:Class rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#bunkbedArtifact">
        <rdfs:subClassOf rdf:resource="http://localhost:8080/BFiaO/EmergencyArtifact#fieldArtifact"/>
        <rdfs:comment xml:lang="en">A bunk bed is one of two single beds usually placed one above the other.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">A bunk bed is one of two single beds usually placed one above the other.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">bunkbedArtifact</rdfs:label>
    </owl:Class>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#busArtifact -->

    <owl:Class rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#busArtifact">
        <rdfs:subClassOf rdf:resource="http://localhost:8080/BFiaO/EmergencyArtifact#groundVehicleArtifact"/>
        <rdfs:comment xml:lang="en">A bus is a large motor vehicle designed to carry passengers.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">A bus is a large motor vehicle designed to carry passengers.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">busArtifact</rdfs:label>
    </owl:Class>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#cBRNArtifact -->

    <owl:Class rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#cBRNArtifact">
        <rdfs:comment xml:lang="en">A CBRN artifact defines personnel an materiel equipment used in CBRN emergencies. For those equipments which needs a vehicle we can find them in the category of Ground Vehicles.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">A CBRN artifact defines personnel an materiel equipment used in CBRN emergencies. For those equipments which needs a vehicle we can find them in the category of Ground Vehicles.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">cBRNArtifact</rdfs:label>
    </owl:Class>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#cBRNIndividualProtectiveArtifact -->

    <owl:Class rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#cBRNIndividualProtectiveArtifact">
        <rdfs:subClassOf rdf:resource="http://localhost:8080/BFiaO/EmergencyArtifact#cBRNArtifact"/>
        <rdfs:comment xml:lang="en">Individual Protective Equipment usually consists of protective clothing, protective masks, decontamination and first-aid kits, chemical detectors and dosimeters in order to provide individuals with protection against CBRN (chemical, biological, radiological and nuclear) agents.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">Individual Protective Equipment usually consists of protective clothing, protective masks, decontamination and first-aid kits, chemical detectors and dosimeters in order to provide individuals with protection against CBRN (chemical, biological, radiological and nuclear) agents.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">cBRNIndividualProtectiveArtifact</rdfs:label>
    </owl:Class>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#cBRNInterventionVehicleArtifact -->

    <owl:Class rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#cBRNInterventionVehicleArtifact">
        <rdfs:subClassOf rdf:resource="http://localhost:8080/BFiaO/EmergencyArtifact#cBRNVehicleArtifact"/>
        <rdfs:comment xml:lang="en">A CBRN Intervention Vehicle is  an vehicle designed for the identification of CBRN aggressive agents and industrial toxics (TIC/TIM). This vehicle has been conceived to rapidly recognize CBRN aggressive agents deriving from toxic substances resulting from natural disasters, industrial disasters, road accidents or terrorist acts.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">A CBRN Intervention Vehicle is  an vehicle designed for the identification of CBRN aggressive agents and industrial toxics (TIC/TIM). This vehicle has been conceived to rapidly recognize CBRN aggressive agents deriving from toxic substances resulting from natural disasters, industrial disasters, road accidents or terrorist acts.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">cBRNInterventionVehicleArtifact</rdfs:label>
    </owl:Class>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#cBRNMaterielDecontaminationArtifact -->

    <owl:Class rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#cBRNMaterielDecontaminationArtifact">
        <rdfs:subClassOf rdf:resource="http://localhost:8080/BFiaO/EmergencyArtifact#cBRNArtifact"/>
        <rdfs:comment xml:lang="en">A CBRN decontamination equipment for materiel is an equipment designed  to remove CBRN components from equipment. The objective of the decontamination is to reduce radiation burden, salvage equipment, and materials, remove loose CBRN contaminants, and fix the remaining in place in preparation for protective storage or permanent disposal work activities.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">A CBRN decontamination equipment for materiel is an equipment designed  to remove CBRN components from equipment. The objective of the decontamination is to reduce radiation burden, salvage equipment, and materials, remove loose CBRN contaminants, and fix the remaining in place in preparation for protective storage or permanent disposal work activities.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">cBRNMaterielDecontaminationArtifact</rdfs:label>
    </owl:Class>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#cBRNMobileLabArtifact -->

    <owl:Class rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#cBRNMobileLabArtifact">
        <rdfs:subClassOf rdf:resource="http://localhost:8080/BFiaO/EmergencyArtifact#cBRNVehicleArtifact"/>
        <rdfs:comment xml:lang="en">A Mobile Lab for CBRN is a truck-transportable laboratory suitable for performing analysis of chemical, nuclearradiological and biological compounds, ensuring that CBRN threats can be rapidlyand accurately identified.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">A Mobile Lab for CBRN is a truck-transportable laboratory suitable for performing analysis of chemical, nuclearradiological and biological compounds, ensuring that CBRN threats can be rapidlyand accurately identified.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">cBRNMobileLabArtifact</rdfs:label>
    </owl:Class>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#cBRNPersonnelDecontaminationArtifact -->

    <owl:Class rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#cBRNPersonnelDecontaminationArtifact">
        <rdfs:subClassOf rdf:resource="http://localhost:8080/BFiaO/EmergencyArtifact#cBRNArtifact"/>
        <rdfs:comment xml:lang="en">A CBRN decontamination equipment for personnel is an equipment designed  to remove CBRN components from people. The objective of the decontamination is to reduce radiation burden, and remove loose CBRN contaminants from personnel.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">A CBRN decontamination equipment for personnel is an equipment designed  to remove CBRN components from people. The objective of the decontamination is to reduce radiation burden, and remove loose CBRN contaminants from personnel.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">cBRNPersonnelDecontaminationArtifact</rdfs:label>
    </owl:Class>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#cBRNReconnaissanceVehicleArtifact -->

    <owl:Class rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#cBRNReconnaissanceVehicleArtifact">
        <rdfs:subClassOf rdf:resource="http://localhost:8080/BFiaO/EmergencyArtifact#cBRNVehicleArtifact"/>
        <rdfs:comment xml:lang="en">A CBRN reconnaissance vehicle conducts surveillance, and route, zone, and area reconnaissance to determine the presence and extent of CBRN contamination using the CBRN reconnaissance techniques of search, survey, surveillance, and sampling.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">A CBRN reconnaissance vehicle conducts surveillance, and route, zone, and area reconnaissance to determine the presence and extent of CBRN contamination using the CBRN reconnaissance techniques of search, survey, surveillance, and sampling.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">cBRNReconnaissanceVehicleArtifact</rdfs:label>
    </owl:Class>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#cBRNVehicleArtifact -->

    <owl:Class rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#cBRNVehicleArtifact">
        <rdfs:subClassOf rdf:resource="http://localhost:8080/BFiaO/EmergencyArtifact#groundVehicleArtifact"/>
        <rdfs:comment xml:lang="en">A CBRN Intervention Vehicle is  an vehicle designed for the identification of CBRN aggressive agents and industrial toxics (TIC/TIM).</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">A CBRN Intervention Vehicle is  an vehicle designed for the identification of CBRN aggressive agents and industrial toxics (TIC/TIM).</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">cBRNVehicleArtifact</rdfs:label>
    </owl:Class>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#caterpillarExcavatorArtifact -->

    <owl:Class rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#caterpillarExcavatorArtifact">
        <rdfs:subClassOf rdf:resource="http://localhost:8080/BFiaO/EmergencyArtifact#groundVehicleArtifact"/>
        <rdfs:comment xml:lang="en">A caterpillar excavator is a bulldozer with linked metal chain fastened around the wheels of a heavy vehicle to help it to move over rough ground.
It is used to carry out earthworks, excavations, demolitions, etc., providing high performance. Being propelled by chains, it can work in places with a steep slope and difficult access.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">A caterpillar excavator is a bulldozer with linked metal chain fastened around the wheels of a heavy vehicle to help it to move over rough ground.
It is used to carry out earthworks, excavations, demolitions, etc., providing high performance. Being propelled by chains, it can work in places with a steep slope and difficult access.</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">caterpillarExcavatorArtifact</rdfs:label>
    </owl:Class>
    


    <!-- http://localhost:8080/BFiaO/EmergencyArtifact#cellsOnWheelsSystemArtifact -->

    <owl:Class rdf:about="http://localhost:8080/BFiaO/EmergencyArtifact#cellsOnWheelsSystemArtifact">
